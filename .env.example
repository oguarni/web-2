# MongoDB Atlas Configuration
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/reservas_db?retryWrites=true&w=majority

# PostgreSQL Configuration (for Sequelize)
POSTGRES_DB=reservas_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# JWT Secret
JWT_SECRET=your-very-secure-secret-key-at-least-32-characters-long

# Session Secret
SESSION_SECRET=your-session-secret-here

# Server Port
PORT=3000

# Environment
NODE_ENV=development